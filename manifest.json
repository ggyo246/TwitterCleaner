{
    "name": "X Cleaner",
    "version": "0.0.0.2",
    "description": "Deletes every tweet of user. Original https://github.com/queeeeeeee/TwitterCleaner",
    "permissions": ["tabs"],
    "content_scripts": [{
        "matches": ["https://*.twitter.com/*"],
        "js": ["contentScript.js"]
    }],
    "action": {
        "default_icon": "icon.png",
        "default_title": "X Cleaner",
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}